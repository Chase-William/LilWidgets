<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="SandboxApp.MasterDetailContainerPage"                  
                  xmlns:pages="clr-namespace:SandboxApp">
  <MasterDetailPage.Master>
    <pages:MasterDetailContainerPageMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage BarBackgroundColor="{StaticResource ColorPrimary}"
                    BarTextColor="{StaticResource ColorContrast}"
                    BackgroundColor="{StaticResource ColorPrimary}">
      <x:Arguments>
        <pages:MasterDetailContainerPageDetail/>
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>